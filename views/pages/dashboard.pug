extends ../layout/main.pug

block header
  .header.bg-primary.pb-6
    .container-fluid
      .header-body
        .row.align-items-center.py-4
          .col-lg-6.col-7
            h6.h2.text-white.d-inline-block.mb-0 Default
            nav.d-none.d-md-inline-block.ml-md-4(aria-label='breadcrumb')
              ol.breadcrumb.breadcrumb-links.breadcrumb-dark
                li.breadcrumb-item
                  a(href='#')
                    i.fas.fa-home
                li.breadcrumb-item
                  a(href='#') Dashboards
                li.breadcrumb-item.active(aria-current='page') Default
          .col-lg-6.col-5.text-right
            a.btn.btn-sm.btn-neutral(href='#') New
            a.btn.btn-sm.btn-neutral(href='#') Filters
        // Card stats
        .row
          .col-xl-3.col-md-6
            .card.card-stats
              // Card body
              .card-body
                .row
                  .col
                    h5.card-title.text-uppercase.text-muted.mb-0 Total traffic
                    span.h2.font-weight-bold.mb-0 350,897
                  .col-auto
                    .icon.icon-shape.bg-gradient-red.text-white.rounded-circle.shadow
                      i.ni.ni-active-40
                p.mt-3.mb-0.text-sm
                  span.text-success.mr-2
                    i.fa.fa-arrow-up
                    |  3.48%
                  span.text-nowrap Since last month
          .col-xl-3.col-md-6
            .card.card-stats
              // Card body
              .card-body
                .row
                  .col
                    h5.card-title.text-uppercase.text-muted.mb-0 New users
                    span.h2.font-weight-bold.mb-0 2,356
                  .col-auto
                    .icon.icon-shape.bg-gradient-orange.text-white.rounded-circle.shadow
                      i.ni.ni-chart-pie-35
                p.mt-3.mb-0.text-sm
                  span.text-success.mr-2
                    i.fa.fa-arrow-up
                    |  3.48%
                  span.text-nowrap Since last month
          .col-xl-3.col-md-6
            .card.card-stats
              // Card body
              .card-body
                .row
                  .col
                    h5.card-title.text-uppercase.text-muted.mb-0 Sales
                    span.h2.font-weight-bold.mb-0 924
                  .col-auto
                    .icon.icon-shape.bg-gradient-green.text-white.rounded-circle.shadow
                      i.ni.ni-money-coins
                p.mt-3.mb-0.text-sm
                  span.text-success.mr-2
                    i.fa.fa-arrow-up
                    |  3.48%
                  span.text-nowrap Since last month
          .col-xl-3.col-md-6
            .card.card-stats
              // Card body
              .card-body
                .row
                  .col
                    h5.card-title.text-uppercase.text-muted.mb-0 Performance
                    span.h2.font-weight-bold.mb-0 49,65%
                  .col-auto
                    .icon.icon-shape.bg-gradient-info.text-white.rounded-circle.shadow
                      i.ni.ni-chart-bar-32
                p.mt-3.mb-0.text-sm
                  span.text-success.mr-2
                    i.fa.fa-arrow-up
                    |  3.48%
                  span.text-nowrap Since last month

block content
  .container-fluid.mt--6
    .row
      .col-xl-8
        .card.bg-default
          .card-header.bg-transparent
            .row.align-items-center
              .col
                h6.text-light.text-uppercase.ls-1.mb-1 Overview
                h5.h3.text-white.mb-0 Sales value
              .col
                ul.nav.nav-pills.justify-content-end
                  li.nav-item.mr-2.mr-md-0(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 20, 10, 30, 15, 40, 20, 60, 60]}]}}' data-prefix='$' data-suffix='k')
                    a.nav-link.py-2.px-3.active(href='#' data-toggle='tab')
                      span.d-none.d-md-block Month
                      span.d-md-none M
                  li.nav-item(data-toggle='chart' data-target='#chart-sales-dark' data-update='{"data":{"datasets":[{"data":[0, 20, 5, 25, 10, 30, 15, 40, 40]}]}}' data-prefix='$' data-suffix='k')
                    a.nav-link.py-2.px-3(href='#' data-toggle='tab')
                      span.d-none.d-md-block Week
                      span.d-md-none W
          .card-body
            // Chart
            .chart
              // Chart wrapper
              canvas#chart-sales-dark.chart-canvas
      .col-xl-4
        .card
          .card-header.bg-transparent
            .row.align-items-center
              .col
                h6.text-uppercase.text-muted.ls-1.mb-1 Performance
                h5.h3.mb-0 Total orders
          .card-body
            // Chart
            .chart
              canvas#chart-bars.chart-canvas
    .row
      .col-xl-8
        .card
          .card-header.border-0
            .row.align-items-center
              .col
                h3.mb-0 Page visits
